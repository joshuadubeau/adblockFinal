{
    "manifest_version": 2,
    "name": "Adblock Extension",
    "version": "0.1",
     "browser_action": {
     },
     "background": {
        "scripts":["background.js"],
        "persistent": false
     },
    "content_scripts": [
      {
        "matches":["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }